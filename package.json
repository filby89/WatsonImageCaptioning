    "cors": "^2.8.4",
    "css-loader": "^0.28.4",
    "drag-drop": "^4.2.0",
    "dropzone": "^5.4.0",
    "express": "^4.14.0",
    "express-fileupload": "^0.4.0",
    "form-data": "^2.3.2",
    "ibm-cloud-env": "^0",
